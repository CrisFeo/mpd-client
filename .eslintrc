{
  "rules": {
    "comma-dangle": [2, "always"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-unused-vars": [
      1,
      {"vars": "all", "args": "after-used"}
    ],
    "quotes": [2, "single"],
    "semi": [2, "always"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}

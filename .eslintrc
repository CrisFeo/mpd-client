{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": ["react"],
  "rules": {
    "comma-dangle": [2, "always"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "quotes": [2, "single"],
    "semi": [2, "always"]
  }
}
